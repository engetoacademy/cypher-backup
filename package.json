    "nexline": "^1.2.0",
    "redis": "^2.8.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "yaml": "^1.10.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/yargs": "^15.0.5",
    "@types/node": "13.1.0",
